{
	"manifest_version": 2,
	"name": "Cheese",
	"version":"1.0.1",
	"icons": {"24": "icon.png"},
	"content_scripts": [
		{
			"matches": [
				"*://*.youtube.com/*"
			],
			"js":["jquery.js","script.js","heatmap.js","jquery-ui.min.js"],
			"css":["styles.css"],
			"run_at": "document_start"
		}
	],
	"background": {
		"scripts":["background.js", "jquery.js"]
	},
	 "browser_action": {
    	"default_title": "Inject!",
			"default_popup": "popup.html"
  	},
  	"permissions": [
      "activeTab",
      "webNavigation",
      "tabs",
		"storage",
		"http://localhost:8080/"
    ]
}
